<%= form_for(@qualification) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_area :name, placeholder: "qualification name" %>
  </div>
  <div class="field">
    <%= f.text_area :school, placeholder: "school" %>
  </div>
  <div class="field">
    <%= f.date_field :completed %>
  </div>
  <%= f.submit "Post", class: "btn btn-primary" %>
<% end %>
